{
    "controllers": {
        "@symfony/ux-turbo": {
            "turbo-core": {
                "enabled": true,
                "fetch": "eager"
            },
            "mercure-turbo-stream": {
                "enabled": true,
                "fetch": "eager"
            }
        },
        "@symfony/ux-live-component": {
            "live": {
                "enabled": true,
                "fetch": "eager"
            }
        },
		"@symfony/ux-dropzone": {
            "dropzone": {
                "enabled": true,
                "fetch": "eager",
                "autoimport": {
                    "@symfony/ux-dropzone/dist/style.min.css": true
                }
            }
        },
		"@symfony/ux-cropperjs": {
			"cropper": {
				"main": "dist/controller.js",
				"webpackMode": "eager",
				"fetch": "eager",
				"enabled": true,
				"autoimport": {
					"cropperjs/dist/cropper.min.css": true,
					"@symfony/ux-cropperjs/dist/style.min.css": true
				}				
			}
		}
    },
    "entrypoints": []
}
