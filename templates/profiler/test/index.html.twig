{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block head %}
	{{ encore_entry_link_tags('app') }}
	{{ encore_entry_script_tags('app') }}
    {{ parent() }}
{% endblock %}


{% block toolbar %}
	{% set icon %}
		<twig:ux:icon name="ion:terminal-outline"></twig:ux:icon>
		<span class="font-monospace">{{ 'test'|u.title }}</span>
	{% endset %}
	
	{% set text %}
		<div>Some test collector</div>
		<div>{{ collector.testData }}</div>
	{% endset %}
	
	{{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { link: true }) }}
{% endblock %}

{% block menu %}
	<div class="d-flex align-items-center">
		<twig:ux:icon name="ion:terminal-outline" width="25px"></twig:ux:icon>
		<span class="font-monospace">Test in the menu side</span>
	</div>
{% endblock %}

{% block panel %}
	<div class="display-1 font-monospace">Швифтанёмся?</div>
{% endblock %}