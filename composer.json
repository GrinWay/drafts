        "vich/uploader-bundle": "^2.4",
        "web-token/jwt-library": "*",
        "wohali/oauth2-discord-new": "^1.2"
    },
	"repositories": [
		{
			"type": "path",
			"url": "./bundles/grinway/service-bundle"
		},
		{
			"type": "path",
			"url": "./bundles/grinway/command-bundle"
		},
		{
			"type": "path",
			"url": "./bundles/grinway/env-processor-bundle"
		},
		{
			"type": "path",
			"url": "./bundles/grinway/web-app-bundle"
		},
		{
			"type": "path",
			"url": "./bundles/grinway/symfony-extensions"
		},
		{
			"type": "path",
			"url": "./bundles/grinway/installer"
		},
		{
			"type": "vcs",
			"url": "https://github.com/GrinWay/Faker.git"
		}
	],
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.2",
        "dbrekelmans/bdi": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.6",
        "phpunit/phpunit": "^11.3|^12",
        "symfony/browser-kit": "7.1.*",
        "symfony/css-selector": "7.1.*",
        "symfony/maker-bundle": "^1.60",
        "symfony/panther": "^2.1",
        "symfony/phpunit-bridge": "^7.1",
        "symfony/stopwatch": "7.1.*",
        "symfony/web-profiler-bundle": "7.1.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": true,
            "grinway/installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "telegram:set_webhook": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
		"app_reinstall_grinway_bundles": [
			"rm -rf ./vendor/grinway",
			"@composer require grinway/command-bundle grinway/env-processor-bundle grinway/service-bundle grinway/web-app-bundle"
		],
		"app_after_composer_install": [
			"@app_fpdf_download",
			"@telegram_set_webhook"
		],
		"watch": "yarn encore dev --watch",
		
        "telegram_set_webhook": [
			"App\\Command\\Telegram\\TelegramSetWebhook"
		],
        "command": [
			"php -r \"echo 'INLINE CLI COMMAND'.\\PHP_EOL;\"",
			"App\\Composer\\Scripts\\Command::execute"
		],
        "pre-package-uninstall": [
			"App\\Composer\\Scripts\\PrePackageUninstall::execute"
		],
        "pre-package-install": [
			"App\\Composer\\Scripts\\PrePackageInstall::execute"
		],
        "post-package-install": [
			"App\\Composer\\Scripts\\PostPackageInstall::execute"
		],
        "pre-install-cmd": [
			"App\\Composer\\Scripts\\PreInstallCmd::execute"
		],
        "post-autoload-dump": [
			"App\\Composer\\Scripts\\PostAutoloadDump::execute"
		],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
		"app_fpdf_download": [
			"App\\Composer\\Scripts\\Download\\FpdfDownloader::download"
		]
    },
    "autoload": {
		"files": [
			"src/Resources/functions.php"
		],
        "psr-4": {
            "App\\": "src/",
            "DataFixtures\\": "fixtures/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
