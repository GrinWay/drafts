{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-session": "*",
    "ajaxray/php-watermark": "^0.1.1",
    "doctrine/dbal": "*",
    "doctrine/doctrine-bundle": "*",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "*",
    "elgigi/commonmark-emoji": "^2.0",
    "embed/embed": "*",
    "endroid/installer": "^1.5",
    "endroid/pdf": "^1.6",
    "endroid/qr-code-bundle": "^6.0",
    "erusev/parsedown": "^1.7",
    "gedmo/doctrine-extensions": "*",
    "gregwar/image": "2",
    "grinway/pure-bundle": "^0.0.0",
    "khanamiryan/qrcode-detector-decoder": "^2.0",
    "knplabs/knp-paginator-bundle": "^6.6",
    "knplabs/knp-snappy": "^1.5",
    "knplabs/knp-snappy-bundle": "^1.10",
    "league/commonmark": "*",
    "league/html-to-markdown": "^5.1",
    "league/uri": "^7.5",
    "lexik/jwt-authentication-bundle": "^3.1",
    "lorenzo/pinky": "^1",
    "makinacorpus/db-tools-bundle": "^1.2",
    "nelmio/cors-bundle": "^2.5",
    "nesbot/carbon": "^3.8",
    "nikic/php-parser": "^5.3",
    "phpcr/phpcr": "*",
    "phpdocumentor/reflection-docblock": "^5.6",
    "phpstan/phpdoc-parser": "^2.0",
    "predis/predis": "^2.3",
    "sensiolabs/ansi-to-html": "^1.2",
    "spomky-labs/otphp": "^11.3",
    "stof/doctrine-extensions-bundle": "^1.12",
    "symfony-cmf/routing": "^3.0",
    "symfony-cmf/routing-bundle": "^3.1",
    "symfony/asset": "7.2.*",
    "symfony/console": "7.2.*",
    "symfony/doctrine-messenger": "7.2.*",
    "symfony/dotenv": "7.2.*",
    "symfony/expression-language": "7.2.*",
    "symfony/flex": "^2",
    "symfony/form": "7.2.*",
    "symfony/framework-bundle": "7.2.*",
    "symfony/html-sanitizer": "7.2.*",
    "symfony/http-client": "7.2.*",
    "symfony/http-foundation": "7.2.*",
    "symfony/intl": "7.2.*",
    "symfony/ldap": "7.2.*",
    "symfony/lock": "7.2.*",
    "symfony/mailer": "7.2.*",
    "symfony/mailgun-mailer": "7.2.*",
    "symfony/mercure-bundle": "*",
    "symfony/messenger": "7.2.*",
    "symfony/mime": "7.2.*",
    "symfony/monolog-bundle": "^3.0",
    "symfony/notifier": "7.2.*",
    "symfony/process": "7.2.*",
    "symfony/property-access": "7.2.*",
    "symfony/property-info": "7.2.*",
    "symfony/rate-limiter": "7.2.*",
    "symfony/runtime": "7.2.*",
    "symfony/security-bundle": "7.2.*",
    "symfony/serializer": "7.2.*",
    "symfony/stimulus-bundle": "^2.22",
    "symfony/string": "7.2.*",
    "symfony/translation": "7.2.*",
    "symfony/twig-bundle": "7.2.*",
    "symfony/ux-icons": "^2.22",
    "symfony/ux-turbo": "^2.22",
    "symfony/ux-twig-component": "^2.22",
    "symfony/validator": "7.2.*",
    "symfony/web-link": "7.2.*",
    "symfony/webpack-encore-bundle": "^2.2",
    "symfony/yaml": "7.2.*",
    "symfonycasts/micro-mapper": "^0.2.1",
    "twig/cssinliner-extra": "^3.16",
    "twig/extra-bundle": "^3.17",
    "twig/inky-extra": "^3.17",
    "twig/string-extra": "*",
    "twig/twig": "^2.12|^3.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/moust/Paint.git"
    },
    {
      "type": "path",
      "url": "bundles/GrinWay/pure-bundle"
    }
  ],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "endroid/installer": true
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-0": {
      "Paint": "vendor/moust/paint/Paint/src/"
    },
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "scripts": {
    "auto-scripts": {
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts",
      "rm -rf ./var/cache"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.2.*"
    }
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^8.2",
    "dbrekelmans/bdi": "^1.4",
    "doctrine/doctrine-fixtures-bundle": "^3.7",
    "mtdowling/jmespath.php": "^2.8",
    "phpunit/phpunit": "11.4",
    "symfony/browser-kit": "7.2.*",
    "symfony/css-selector": "7.2.*",
    "symfony/debug-bundle": "7.2.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/panther": "^2.1",
    "symfony/phpunit-bridge": "^7.2",
    "symfony/stopwatch": "7.2.*",
    "symfony/var-dumper": "7.2.*",
    "symfony/web-profiler-bundle": "7.2.*",
    "zenstruck/browser": "^1.9",
    "zenstruck/foundry": "^2.3"
  }
}
